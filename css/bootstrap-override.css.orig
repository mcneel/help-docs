/*
##############################################################################
## Image float overrides. Allows to wrap text around images.
## {: .float-img-right}
## To finish the wrapping, insert this text under next block object:
## {: .clear-img}
## That assures the next block element will stay clear of the image.
##############################################################################
*/
.float-img-right {
  float: right;
  padding-left: 25px;
  padding-top: 5px;
  padding-bottom: 5px;
}

.float-img-left {
  float: left;
  padding-right: 25px;
  padding-top: 5px;
  padding-bottom: 5px;
}

.clear-img {
  clear:both;
}

