  font-size: 15px;
  line-height: 1.3;
  font-weight: 400 !important;
  font-style: normal;
  font-variant: normal;
  color: #111;
  background-color: white;
  -webkit-text-size-adjust: 100%;
}
@media screen and (max-width: 450px) {
